{
  "name": "Task 1 Epam second attemp",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.16.2",
    "@wdio/cli": "^9.14.0",
    "@wdio/cucumber-framework": "^9.16.2",
    "@wdio/local-runner": "^9.14.0",
    "@wdio/spec-reporter": "^9.14.0"
  },
  "scripts": {
    "ui-tests": "wdio run src/configs/wdio.conf.js --cucumberOpts.tagExpression='@ui-test'",
    "wdio": "wdio run ./src/configs/wdio.conf.js",
    "tags": "wdio run ./src/configs/wdio.conf.js --cucumber.Opts.tags='@ui-test'",
    "allure:generate": "allure generate ./src/configs/logs/allure-results --clean -o ./src/configs/logs/allure-report",
    "allure:open": "allure open ./src/configs/logs/allure-report"
  }
}
